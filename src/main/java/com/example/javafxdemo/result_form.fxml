<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox xmlns="http://javafx.com/javafx/1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.example.javafxdemo.HelloController">
   <children>
      <VBox alignment="CENTER" prefHeight="202.0" prefWidth="169.0">
         <children>
            <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="18.0" prefWidth="187.0" text="There is nothing to look at!" textAlignment="CENTER" />
            <Button fx:id="Back" mnemonicParsing="false" onAction="#back" text="Go Back">
               <font>
                  <Font name="Arial Unicode MS" size="12.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</VBox>
